{% if footermenu %}
	<ul>
	{% for item in footermenu %}
		<li class="{{ item.classes | join(' ') }}">
			<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
			{% include "/footer/footer_menu.twig" with {'footermenu': item.get_children} %}
		</li>
	{% endfor %}
	</ul>
{% endif %}
